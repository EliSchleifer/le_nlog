<?xml version="1.0"?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <extensions>
    <add assembly="LeTarget"/>
  </extensions>
  <targets>
    <target name="logentries" type="Logentries" key="ac3dc54c-2084-4c00-9792-2de17e0043bc" location="localhost6/nlog.log"
            layout="${date:format=ddd MMM dd HH\:mm\:ss zzz yyyy} ${logger} : ${level:uppercase=true}, ${message}"/>
  </targets>
  <rules>
    <logger name="*" minLevel="Info" appendTo="logentries"/>
  </rules>
</nlog>
